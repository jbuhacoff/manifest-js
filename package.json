{
  "name": "@jbuhacoff/manifest",
  "version": "0.2.0",
  "description": "Automates and coordinates actions on repositories",
  "files": [
    "src/manifest.js"
  ],
  "main": "src/manifest",
  "preferGlobal": "true",
  "bin": {
    "manifest": "src/manifest.js"
  },
  "scripts": {
