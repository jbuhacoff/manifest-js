    "url": "https://github.com/jbuhacoff"
  },
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/jbuhacoff/manifest-js/issues"
  },
  "homepage": "https://github.com/jbuhacoff/manifest-js#readme",
  "dependencies": {
    "deasync": "^0.1.15",
    "js-yaml": "^3.13.1",
    "simple-git": "^1.116.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@commitlint/utils": "^8.0.0",
    "@jbuhacoff/commitlint-config-type-enum": "^1.0.0"
